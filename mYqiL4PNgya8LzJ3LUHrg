FROM python:3.11-slim

# Install system dependencies
RUN apt-get update &amp;&amp; apt-get install -y \
    curl wget gnupg unzip fonts-liberation libnss3 libatk-bridge2.0-0 libxss1 libasound2 libx11-xcb1 libxcomposite1 libxdamage1 libxrandr2 libgbm1 libgtk-3-0 \
    &amp;&amp; rm -rf /var/lib/apt/lists/*

# Install Playwright and dependencies
RUN pip install --upgrade pip
COPY requirements.txt .
RUN pip install -r requirements.txt
RUN playwright install --with-deps

# Copy project files
WORKDIR /app
COPY . .

# Set environment variables
ENV PYTHONUNBUFFERED=1

# Entry point
CMD ["python", "main.py"]
</code></pre></div></div></div><div class="relative pb-6 w-full after:border-b after:border-stroke-300 after:w-full after:absolute after:mt-3"></div><h2 class="text-xl-medium [&amp;:not(:first-child)]:pt-3.5 pb-1"> <code class="font-ligatures-none mx-0.5 rounded-[4px] border border-stroke-300 bg-accent-200/60 px-1 py-px font-mono text-foreground-800 text-sm dark:bg-accent-200" style="opacity: 1;">fingerprint_audit.md</code></h2><div class="rounded-xl dark:border dark:border-stroke-250" style="opacity: 1;"><div class="flex w-full items-center justify-between border-b border-white/20 bg-background-static-850 py-2 pe-2.5 ps-4 text-foreground-static-250 text-sm dark:border-stroke-250 dark:bg-background-static-900 rounded-t-xl"><div class="flex items-center gap-2"><span class="capitalize">markdown</span><button class="relative min-h-9 min-w-9 rounded-xl bg-transparent p-2 hover:bg-white/8 active:bg-white/5" type="button" title="Collapse code snippet" aria-expanded="true"><svg viewBox="0 0 24 24" fill="currentColor" xmlns="http://www.w3.org/2000/svg" class="size-4 transition-transform -rotate-90"><path d="M8.46967 4.21967C8.17678 4.51256 8.17678 4.98744 8.46967 5.28033L15.1893 12L8.46967 18.7197C8.17678 19.0126 8.17678 19.4874 8.46967 19.7803C8.76256 20.0732 9.23744 20.0732 9.53033 19.7803L16.7803 12.5303C17.0732 12.2374 17.0732 11.7626 16.7803 11.4697L9.53033 4.21967C9.23744 3.92678 8.76256 3.92678 8.46967 4.21967Z"></path></svg></button></div><div class="flex items-center gap-2"><button class="relative min-h-9 min-w-9 gap-1.5 rounded-xl bg-transparent p-2 hover:bg-white/8 active:bg-white/5" type="button" title="Copy code" data-copy="false"><div class="flex items-center justify-center gap-2" aria-hidden="true"><svg viewBox="0 0 24 24" fill="currentColor" xmlns="http://www.w3.org/2000/svg" class="w-5"><path d="M5.5028 4.62704L5.5 6.75V17.2542C5.5 19.0491 6.95507 20.5042 8.75 20.5042L17.3663 20.5045C17.0573 21.3782 16.224 22.0042 15.2444 22.0042H8.75C6.12665 22.0042 4 19.8